{
  "compilerOptions": {
    "allowJs": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "module": "esNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "target": "esnext",
    "lib": [
      "dom",
      "es5",
      "es6",
      "es7",
      "es2017"
    ],
    "skipLibCheck": true,
    "removeComments": true,
    "baseUrl": "./",
    "paths": {
      "lib/*": [
        "lib/*"
      ],
    },
    "noImplicitAny": true,
    "strict": true,
    "resolveJsonModule": true,
    "outDir": "./"
  },
  "exclude": ["node_modules"]
}
  